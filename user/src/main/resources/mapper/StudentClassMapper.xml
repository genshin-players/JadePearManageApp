<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.mapper.StudentClassMapper">

<select id="selectStudentClassById" resultType="cn.bdqn.entity.StudentClass">
    SELECT * FROM `student_class`  WHERE `student_id`=#{id}
    </select>
</mapper>
